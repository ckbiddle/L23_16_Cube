  }  

  // any green in the color?
  if ( pColor == 2 || pColor == 4 || pColor == 5 || pColor == 7 )
  {
    greenOn = true;
  }

  // any blue in the color?
  if ( pColor == 3 || pColor == 5 || pColor == 6 || pColor == 7 )
  {
    blueOn = true;
  }

  // byte dataArray[MAX_FRAMES][NUM_COLORS][NUM_PANELS][NUM_ROWS][BYTES_PER_ROW];
  //                     ^           ^           ^           ^         ^
  //                     |           |           |           |         |
  //                     |           |           |           |         +----- First 8 bits, second 8 bits (x-axis)
  //                     |           |           |           +-------- Row (z-axis)
  //                     |           |           +------------ Panel number (y-axis)
  //                     |           +--------------- Red data, Green data, Blue data
  //                     +------------------ Animation frame sequence number

  for ( byte panelIndex = 0; panelIndex < NUM_PANELS; panelIndex++ )
  {
    for ( byte rowIndex = 0; rowIndex < NUM_ROWS; rowIndex++ )
    {
      for ( byte byteIndex = 0; byteIndex < BYTES_PER_ROW; byteIndex++ )
      {
        if ( redOn )
        {
          dataArray[0][0][panelIndex][rowIndex][byteIndex] = B11111111;  
        }

        if ( greenOn )
        {
          dataArray[0][1][panelIndex][rowIndex][byteIndex] = B11111111;  
        }

        if ( blueOn )
        {
          dataArray[0][2][panelIndex][rowIndex][byteIndex] = B11111111;  
        }
      }
    }
  }    
}

void loadTelescopingBoxes()
{
  // clear out the array
  for ( int frameIndex = 0; frameIndex < MAX_FRAMES; frameIndex++ )
  {
    for ( int colorIndex = 0; colorIndex < NUM_COLORS; colorIndex++ )
    {
      for ( int panelIndex = 0; panelIndex < NUM_PANELS; panelIndex++ )
      {
        for ( int rowIndex = 0; rowIndex < NUM_ROWS; rowIndex++ )
        {
          dataArray[frameIndex][colorIndex][panelIndex][rowIndex][0] = B00000000;
          dataArray[frameIndex][colorIndex][panelIndex][rowIndex][1] = B00000000;
        }
      }
    }
  }

  double zCoord = 0.0;
  
  // Load white inner box.
  for ( int frameIndex = 0; frameIndex < 28; frameIndex++ )
  {
    if ( frameIndex < 8 )
