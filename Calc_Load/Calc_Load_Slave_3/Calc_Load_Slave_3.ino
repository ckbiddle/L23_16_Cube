void loadHouse( byte pPanelPairId )
{
  // clear out the array
  for ( int frameIndex = 0; frameIndex < MAX_FRAMES; frameIndex++ )
  {
    for ( int colorIndex = 0; colorIndex < NUM_COLORS; colorIndex++ )
    {
      for ( int panelIndex = 0; panelIndex < NUM_PANELS; panelIndex++ )
      {
        for ( int rowIndex = 0; rowIndex < NUM_ROWS; rowIndex++ )
        {
          dataArray[frameIndex][colorIndex][panelIndex][rowIndex][0] = B00000000;
          dataArray[frameIndex][colorIndex][panelIndex][rowIndex][1] = B00000000;
        }
      }
    }
  }

  // Loop through the houseWireFrame array, each element of which represents
  // a line making up the house, and draw the entire house in each frame,
  // rotating 2PI/32 radians in each successive frame

  for ( byte frameIndex = 0; frameIndex < 32; frameIndex++ )
  {
    for ( byte houseIndex = 0; houseIndex < HOUSE_WIRE_FRAME_SIZE; houseIndex++ )
    {
      float *line = (float *) houseWireFrame + houseIndex * 7;
  
      int color   = (int) pgm_read_float((float *) line );

      // Why the 7.5 offset? Doing this in conjunction with passing +7.5 into drawLine()
      // for pOffsetX and pOffsetY moves the z-rotation axis from (x,y) = (0,0) to
      // (x,y) = (7.5,7.5). This way, the house will rotate about the center of the cube.
  
      double xStart = (double) pgm_read_float((float *) line + 1 ) - 7.5;
      double yStart = (double) pgm_read_float((float *) line + 2 ) - 7.5;
      double zStart = (double) pgm_read_float((float *) line + 3 );
  
      double xEnd   = (double) pgm_read_float((float *) line + 4 ) - 7.5;
      double yEnd   = (double) pgm_read_float((float *) line + 5 ) - 7.5;
      double zEnd   = (double) pgm_read_float((float *) line + 6 );
  
      drawLine(
        color,          // byte pColor
        frameIndex,     // byte pFrameIndex
        pPanelPairId,   // byte pPanelPairId
        xStart,         // double pStartXcoord
        yStart,         // double pStartYcoord
        zStart,         // double pStartZcoord
        xEnd,           // double pEndXcoord
        yEnd,           // double pEndYcoord
        zEnd,           // double pEndZcoord
        frameIndex,     // int pRotationStep (matches frameIndex in this case)
        7.5,            // double pOffsetX
        7.5             // double pOffsetY
      );
    }
  }
}

