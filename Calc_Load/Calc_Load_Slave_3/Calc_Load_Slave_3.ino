void setup()
{
  pinMode( LATCH_PIN, OUTPUT );
  pinMode( CLOCK_PIN, OUTPUT );

  // Panel 1  
  pinMode( RED_DATA_PIN_PNL_1, OUTPUT );
  pinMode( GREEN_DATA_PIN_PNL_1, OUTPUT );
  pinMode( BLUE_DATA_PIN_PNL_1, OUTPUT );
  
  // Panel 2  
  pinMode( RED_DATA_PIN_PNL_2, OUTPUT );
  pinMode( GREEN_DATA_PIN_PNL_2, OUTPUT );
  pinMode( BLUE_DATA_PIN_PNL_2, OUTPUT );

  for ( int cathodePin = STARTING_CATHODE_PIN; cathodePin < STARTING_CATHODE_PIN + NUM_ROWS; cathodePin++ )
  {
    pinMode( cathodePin, OUTPUT );
    digitalWrite( cathodePin, LOW );
  }

  digitalWrite( activeLayer, HIGH );

  Wire.begin( SLAVE_ADDRESS );      // join i2c bus with SLAVE_ADDRESS
  Wire.onReceive( receiveEvent );   // register event

  frameIndex = 0;

  // Initialize array
  // byte dataArray[MAX_FRAMES][NUM_COLORS][NUM_PANELS][NUM_ROWS][BYTES_PER_ROW];
  for ( byte frmInx = 0; frmInx < MAX_FRAMES; frmInx++ )
  {
    for ( byte clrInx = 0; clrInx < NUM_COLORS; clrInx++ )
    {
      for ( byte pnlInx = 0; pnlInx < NUM_PANELS; pnlInx++ )
      {
        for ( byte rowInx = 0; rowInx < NUM_ROWS; rowInx++ )
        {
          dataArray[frmInx][clrInx][pnlInx][rowInx][0] = B00000000;
          dataArray[frmInx][clrInx][pnlInx][rowInx][1] = B00000000;
        }
      }
    }
  }
    
  // Serial.begin( 9600 );
}

void loop()
{
  if ( mode == MODE_STANDARD_PLAY )
  {
    flashAnimationFrame( frameIndex );
  }
  else if ( mode == MODE_KINETIC_BALL || mode == MODE_WHITE_BALL || mode == MODE_3D_PONG )
  {
    flashAnimationFrame( 0 );
  }
  else if ( mode == MODE_SCROLL_PANEL_BACK )
  {
    // This mode assumes that you have loaded dataArray[0] and dataArray[1] with what
    // you want displayed when this panel falls in line with the frame index.
    if ( frameIndex == 2 * ( thisPanel - 1 ))
    {
      flashAnimationFrame( 0 );
    }
    else if ( frameIndex == 2 * ( thisPanel - 1 ) + 1 )
    {
      flashAnimationFrame( 1 );
    }
    else
    {
      flashAnimationFrame( 2 );  // empty frame    
    }
  }
  else if ( mode == MODE_SCROLL_RIGHT_LEFT )
  {
    // If this is the rear panel continue to flash the logo
    // that should be in dataArray[1]
    if ( thisPanel == 8 )
    {
      flashAnimationFrame( 1 );
    }
    else
    {
      flashAnimationFrame( 2 );
    }
  }
} 

