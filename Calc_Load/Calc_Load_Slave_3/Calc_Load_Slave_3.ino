void flashAnimationFrame( int currentFrame )
{
  int adjustedLayer = 0;

  for ( int currentLayer = 0; currentLayer < NUM_ROWS; currentLayer++ )
  {
    digitalWrite( STARTING_CATHODE_PIN + currentLayer, HIGH );
      
    // Ground LATCH_PIN and hold low for as long as you are transmitting
    digitalWrite( LATCH_PIN, 0 );

    if ( currentLayer + LAYER_PHASE_CORRECTION < NUM_ROWS )
    {
      adjustedLayer = currentLayer + LAYER_PHASE_CORRECTION;
    }
    else
    {
      adjustedLayer = currentLayer - NUM_ROWS + LAYER_PHASE_CORRECTION;
    }
    
    shiftOutRGB( dataArray[currentFrame][0][PANEL_1_INDEX][adjustedLayer][0], dataArray[currentFrame][1][PANEL_1_INDEX][adjustedLayer][0], dataArray[currentFrame][2][PANEL_1_INDEX][adjustedLayer][0],
                 dataArray[currentFrame][0][PANEL_2_INDEX][adjustedLayer][0], dataArray[currentFrame][1][PANEL_2_INDEX][adjustedLayer][0], dataArray[currentFrame][2][PANEL_2_INDEX][adjustedLayer][0]
               );

    shiftOutRGB( dataArray[currentFrame][0][PANEL_1_INDEX][adjustedLayer][1], dataArray[currentFrame][1][PANEL_1_INDEX][adjustedLayer][1], dataArray[currentFrame][2][PANEL_1_INDEX][adjustedLayer][1],
                 dataArray[currentFrame][0][PANEL_2_INDEX][adjustedLayer][1], dataArray[currentFrame][1][PANEL_2_INDEX][adjustedLayer][1], dataArray[currentFrame][2][PANEL_2_INDEX][adjustedLayer][1]
               );

    // return the latch pin high to signal chip that it 
    // no longer needs to listen for information    
    digitalWrite( LATCH_PIN, 1 );

    // Get rid of ghosting.
    // digitalWrite( LATCH_PIN, 0 );
    // shiftOutRGB( 0x00, 0x00, 0x00, 0x00, 0x00, 0x00 );
    // shiftOutRGB( 0x00, 0x00, 0x00, 0x00, 0x00, 0x00 );
    // digitalWrite( LATCH_PIN, 1 );

    digitalWrite( STARTING_CATHODE_PIN + currentLayer, LOW );
  }
}

void shiftOutRGB( byte panel1RedData,
                  byte panel1GreenData,
                  byte panel1BlueData,
                  byte panel2RedData,
                  byte panel2GreenData,
                  byte panel2BlueData
                )
{
  int inx = 0;
  int pinState;

  //clear everything out just in case to
  //prepare shift register for bit shifting

  // Panel 1  
  digitalWrite( RED_DATA_PIN_PNL_1, 0 );
  digitalWrite( GREEN_DATA_PIN_PNL_1, 0 );
  digitalWrite( BLUE_DATA_PIN_PNL_1, 0 );

  // Panel 2  
  digitalWrite( RED_DATA_PIN_PNL_2, 0 );
  digitalWrite( GREEN_DATA_PIN_PNL_2, 0 );
  digitalWrite( BLUE_DATA_PIN_PNL_2, 0 );
  
//  digitalWrite( CLOCK_PIN, 0 );

  for ( inx = 7; inx >= 0; inx-- )
  {
    digitalWrite( CLOCK_PIN, 0 );

    // if the value passed to myDataOut and a bitmask result 
    // true then... so if we are at i=6 and our value is
    // %11010100 it would the code compares it to %01000000 
    // and proceeds to set pinState to 1.

    pinState = 0;
    
    if ( panel1RedData & ( 1 << inx ))
    {
      pinState = 1;
    }

    // Sets the pin to HIGH or LOW depending on pinState
    digitalWrite( RED_DATA_PIN_PNL_1, pinState );

    pinState= 0;
    
    if ( panel2RedData & ( 1 << inx ))
    {
      pinState = 1;
    }

    // Sets the pin to HIGH or LOW depending on pinState
    digitalWrite( RED_DATA_PIN_PNL_2, pinState );

    pinState = 0;

    if ( panel1GreenData & ( 1 << inx ))
    {
      pinState = 1;
    }
    
    digitalWrite( GREEN_DATA_PIN_PNL_1, pinState );

    pinState = 0;

    if ( panel2GreenData & ( 1 << inx ))
    {
      pinState = 1;
    }
    
    digitalWrite( GREEN_DATA_PIN_PNL_2, pinState );

    pinState = 0;

    if ( panel1BlueData & ( 1 << inx ))
    {
      pinState = 1;
    }
    
    digitalWrite( BLUE_DATA_PIN_PNL_1, pinState );

    pinState = 0;

    if ( panel2BlueData & ( 1 << inx ))
    {
      pinState = 1;
    }
    
    digitalWrite( BLUE_DATA_PIN_PNL_2, pinState );

    // register shifts bits on upstroke of clock pin  
    digitalWrite( CLOCK_PIN, 1 );
    
    // zero the data pin after shift to prevent bleed through
    digitalWrite( RED_DATA_PIN_PNL_1, 0 );
    digitalWrite( GREEN_DATA_PIN_PNL_1, 0 );
    digitalWrite( BLUE_DATA_PIN_PNL_1, 0 );
    digitalWrite( RED_DATA_PIN_PNL_2, 0 );
    digitalWrite( GREEN_DATA_PIN_PNL_2, 0 );
    digitalWrite( BLUE_DATA_PIN_PNL_2, 0 );
  }

  //stop shifting
  digitalWrite( CLOCK_PIN, 0 );
}

