    loadDinerEnBlanc( thisPanel );
  }
  else if ( mode == MODE_LOAD_WHITE_HELIX )
  {
    loadWhiteHelix( thisPanel );
  }
  else if ( mode == MODE_LOAD_WHITE_WAVES )
  {
    loadWhiteWaves( thisPanel );
  }
  else if ( mode == MODE_LOAD_JEWEL )
  {
    loadJewel( thisPanel );
  }
  else if ( mode == MODE_LOAD_TEL_BOXES )
  {
    loadTelescopingBoxes();
  }
  else if ( mode == MODE_LOAD_OBRA )
  {
    loadObra();    
  }
  else if ( mode == MODE_LOAD_LIGHTWAV )
  {
    loadLightWav();    
  }
  else if ( mode == MODE_3D_PONG )
  {
    // This is what is on the master side for the 3D pong mode. The paddles only
    // need the x an z coordinates. The y coordinates never change, always 0
    // for the front, and 15 for the rear.

    //   Wire.beginTransmission( SLAVE_ADDRESS );
    //   Wire.write( mode );
    //   Wire.write((byte) ( pongBallXcoord + 0.5 ));
    //   Wire.write((byte) ( pongBallYcoord + 0.5 ));
    //   Wire.write((byte) ( pongBallZcoord + 0.5 ));
    //   Wire.write( pongMode );
    //   Wire.write( paddleCoords[0][0] );  Front paddle x-coordinate
    //   Wire.write( paddleCoords[0][1] );  Front paddle z-coordinate
    //   Wire.write( paddleCoords[1][0] );  Rear paddle x-coordinate
    //   Wire.write( paddleCoords[1][1] );  Rear paddle z-coordinate
    //   Wire.endTransmission();
  
    // We're going to borrow frame index 0 of the data buffer so we don't
    // create any more arrays taking up memory. We're going to do a WHITE ball.
    // dataArray[MAX_FRAMES][NUM_COLORS][NUM_PANELS][NUM_ROWS][BYTES_PER_ROW]

    // clear out what was in frame index 0 of the array previously
    for ( byte zIndex = 0; zIndex < NUM_ROWS; zIndex++ )
    {
      for ( byte colorIndex = 0; colorIndex < NUM_COLORS; colorIndex++ )
      {      
        dataArray[0][colorIndex][0][zIndex][0] = B00000000;
        dataArray[0][colorIndex][0][zIndex][1] = B00000000;
        dataArray[0][colorIndex][1][zIndex][0] = B00000000;
        dataArray[0][colorIndex][1][zIndex][1] = B00000000;
      }
    }

    xCoord = Wire.read();
    yCoord = Wire.read();
    zCoord = Wire.read();
    pongMode = Wire.read();
    frontPaddle_xCoord = Wire.read();
    frontPaddle_zCoord = Wire.read();
    rearPaddle_xCoord = Wire.read();
    rearPaddle_zCoord = Wire.read();

    // compute what the y coordinates would be of the two panels in this pair.
    byte yCoordIndex1 = ( PANEL_PAIR_ID - 1 ) * 2;
    byte yCoordIndex2 = ( PANEL_PAIR_ID - 1 ) * 2 + 1;

    // for ( byte ballZindex = 0; ballZindex < 5; ballZindex++ )
    for ( byte ballZindex = 0; ballZindex < 3; ballZindex++ )
    {
      // if any part of the 3 voxel width ball falls within the range of the panels
      // in this panel pair, then we need to populate the panel data with appropriate
      // parts of the ball. Otherwise, just ignore

      // if the ball is within panel 1      
      if ( yCoord + 1 == yCoordIndex1 || yCoord - 1 == yCoordIndex1 || yCoord == yCoordIndex1 )
      {
        // if the z coordinate of the data array is within the ball's boundaries
        // if ( zCoord - 2 + ballZindex >= 0 && zCoord - 2 + ballZindex < 16 )
        if ( zCoord - 1 + ballZindex >= 0 && zCoord - 1 + ballZindex < 16 )
        {
          // bit shift as needed to place the ball horizontally in the correct position
          // if ( xCoord - 2 >= 0 )
          if ( xCoord - 1 >= 0 )
          {
            dataArray[0][0][0][zCoord - 1 + ballZindex][0] = pongBallSeed[ballZindex] >> ( xCoord - 1 );
            dataArray[0][1][0][zCoord - 1 + ballZindex][0] = pongBallSeed[ballZindex] >> ( xCoord - 1 );
            dataArray[0][2][0][zCoord - 1 + ballZindex][0] = pongBallSeed[ballZindex] >> ( xCoord - 1 );
          }
          else
          {
            dataArray[0][0][0][zCoord - 1 + ballZindex][0] = pongBallSeed[ballZindex] << ( 1 - xCoord );
            dataArray[0][1][0][zCoord - 1 + ballZindex][0] = pongBallSeed[ballZindex] << ( 1 - xCoord );
            dataArray[0][2][0][zCoord - 1 + ballZindex][0] = pongBallSeed[ballZindex] << ( 1 - xCoord );
          }

          // if ( 10 - xCoord >= 0 )
          if ( 9 - xCoord >= 0 )
          {            
            dataArray[0][0][0][zCoord - 1 + ballZindex][1] = pongBallSeed[ballZindex] << ( 9 - xCoord );
            dataArray[0][1][0][zCoord - 1 + ballZindex][1] = pongBallSeed[ballZindex] << ( 9 - xCoord );
            dataArray[0][2][0][zCoord - 1 + ballZindex][1] = pongBallSeed[ballZindex] << ( 9 - xCoord );
          }
          else
          {
            dataArray[0][0][0][zCoord - 1 + ballZindex][1] = pongBallSeed[ballZindex] >> ( xCoord - 9 );
            dataArray[0][1][0][zCoord - 1 + ballZindex][1] = pongBallSeed[ballZindex] >> ( xCoord - 9 );
            dataArray[0][2][0][zCoord - 1 + ballZindex][1] = pongBallSeed[ballZindex] >> ( xCoord - 9 );
          }
        }
      }
      
      // if the ball is within panel 2      
      if ( yCoord + 1 == yCoordIndex2 || yCoord - 1 == yCoordIndex2 || yCoord == yCoordIndex2 )
      {
        // if the z coordinate of the data array is within its bounds
        // if ( zCoord - 2 + ballZindex >= 0 && zCoord - 2 + ballZindex < 16 )
        if ( zCoord - 1 + ballZindex >= 0 && zCoord - 1 + ballZindex < 16 )
        {
          // pongBallSeed[1] is the fatter part of the ball
          // if ( xCoord - 2 >= 0 )
          if ( xCoord - 1 >= 0 )
          {
            dataArray[0][0][1][zCoord - 1 + ballZindex][0] = pongBallSeed[ballZindex] >> ( xCoord - 1 );
            dataArray[0][1][1][zCoord - 1 + ballZindex][0] = pongBallSeed[ballZindex] >> ( xCoord - 1 );
            dataArray[0][2][1][zCoord - 1 + ballZindex][0] = pongBallSeed[ballZindex] >> ( xCoord - 1 );
          }
          else
          {
            dataArray[0][0][1][zCoord - 1 + ballZindex][0] = pongBallSeed[ballZindex] << ( 1 - xCoord );
            dataArray[0][1][1][zCoord - 1 + ballZindex][0] = pongBallSeed[ballZindex] << ( 1 - xCoord );
            dataArray[0][2][1][zCoord - 1 + ballZindex][0] = pongBallSeed[ballZindex] << ( 1 - xCoord );
          }          
            
          if ( 9 - xCoord >= 0 )
          {          
            dataArray[0][0][1][zCoord - 1 + ballZindex][1] = pongBallSeed[ballZindex] << ( 9 - xCoord );
            dataArray[0][1][1][zCoord - 1 + ballZindex][1] = pongBallSeed[ballZindex] << ( 9 - xCoord );
            dataArray[0][2][1][zCoord - 1 + ballZindex][1] = pongBallSeed[ballZindex] << ( 9 - xCoord );
          }
          else
          {
            dataArray[0][0][1][zCoord - 1 + ballZindex][1] = pongBallSeed[ballZindex] >> ( xCoord - 9 ); 
            dataArray[0][1][1][zCoord - 1 + ballZindex][1] = pongBallSeed[ballZindex] >> ( xCoord - 9 ); 
            dataArray[0][2][1][zCoord - 1 + ballZindex][1] = pongBallSeed[ballZindex] >> ( xCoord - 9 ); 
          }
        }
      }
    } // end for ( byte ballZindex = 0; ballZindex < 3; ballZindex++ )

    if ( PANEL_PAIR_ID == 1 )
    {
      // render paddle 1 in the frontmost panel (y coordinate 0)
      renderPaddle( 'F' );
    }
    else if ( PANEL_PAIR_ID == 8 )
    {
      // render paddle 2 in the rearmost panel (y coordinate 15)
      renderPaddle( 'B' );
    }
  } // end else if ( mode == MODE_3D_PONG )
}

void renderPaddle( char pPosition )  // F for "Front", B for "Back"
{
  byte paddleHeight = 4;
  int rearPaddleZstartIndex = rearPaddle_zCoord - ( paddleHeight / 2 );
  int frontPaddleZstartIndex = frontPaddle_zCoord - ( paddleHeight / 2 );
  byte paddleFragment = B11111000;

  if ( pPosition == 'F' )
  {
    // Render the paddle in front if in multi-player, single-player, or pong demo mode
    if ( pongMode == B10010000 || pongMode == B10000000 || pongMode == B10100000 )
    {
      for ( int inx = frontPaddleZstartIndex; inx < frontPaddleZstartIndex + paddleHeight + 1; inx++ )
      {
        if ( inx >= 0 && inx < NUM_ROWS )
        {
          if ( frontPaddle_xCoord - 2 >= 0 )
          {
            dataArray[0][1][0][inx][0] = paddleFragment >> ( frontPaddle_xCoord - 2 );
          }
          else
          {
            dataArray[0][1][0][inx][0] = paddleFragment << ( 2 - frontPaddle_xCoord );
          }

          if ( 10 - frontPaddle_xCoord >= 0 )
          {
            dataArray[0][1][0][inx][1] = paddleFragment << ( 10 - frontPaddle_xCoord );
          }
          else
          {
            dataArray[0][1][0][inx][1] = paddleFragment >> ( frontPaddle_xCoord - 10 );
          }
        }
      }        
    }  
  }  
  else if ( pPosition == 'B' )
  {
    // Only render the paddle in back if in multi-player or pong demo mode
    if ( pongMode == B10010000 || pongMode == B10100000 )
    {
      for ( int inx = rearPaddleZstartIndex; inx < rearPaddleZstartIndex + paddleHeight + 1; inx++ )
      {
        if ( inx >= 0 && inx < NUM_ROWS )
        {
          if ( rearPaddle_xCoord - 2 >= 0 )
          {
            dataArray[0][1][1][inx][0] = paddleFragment >> ( rearPaddle_xCoord - 2 );
          }
          else
          {
            dataArray[0][1][1][inx][0] = paddleFragment << ( 2 - rearPaddle_xCoord );
          }
          
          if ( 10 - rearPaddle_xCoord >= 0 )
          {
            dataArray[0][1][1][inx][1] = paddleFragment << ( 10 - rearPaddle_xCoord );
          }
          else
          {
            dataArray[0][1][1][inx][1] = paddleFragment >> ( rearPaddle_xCoord - 10 );
          }
        }
      }
    }
  }
}

void shiftInData( int currentFrame )
{
  // For this to work, we assume that the data to be scrolled
  // is stored consecutively starting in dataArray[3] and
  // that dataArray[2] is empty. We will left shift the data
  // from dataArray[3] and upwards into dataArray[2] and
  // continously play dataArray[2]

  // dataArray[MAX_FRAMES][NUM_COLORS][NUM_PANELS][NUM_ROWS][BYTES_PER_ROW]

  byte leftPaneIndex = (( currentFrame + 8 ) - mod(( currentFrame + 8 ), 16 )) / 16 + 2;
  byte leftByteIndex = mod((( currentFrame + 8 ) - mod(( currentFrame + 8 ), 8 )) / 8, 2 );

  byte rightPaneIndex = ( currentFrame - mod( currentFrame, 16 )) / 16 + 3;
  byte rightByteIndex = mod(( currentFrame - mod( currentFrame, 8 )) / 8, 2 );

  byte leftShiftIndex = mod( currentFrame, 8 ) + 1;

  for ( int rowIndex = 0; rowIndex < NUM_ROWS; rowIndex++ )
  {
    for ( int colorIndex = 0; colorIndex < NUM_COLORS; colorIndex++ )
    {
      for ( int panelIndex = 0; panelIndex < NUM_PANELS; panelIndex++ )
      {
        byte tempLeftOriginal  = B00000000;
        byte tempRightOriginal = B00000000;
        byte tempLeftNew       = B00000000;
        byte tempRightNew      = B00000000;

        // shift over what's in there now
        tempLeftOriginal  = dataArray[2][colorIndex][panelIndex][rowIndex][0] << 1;
        tempRightOriginal = dataArray[2][colorIndex][panelIndex][rowIndex][1] << 1;

        // create the new stuff to be added        
        tempLeftNew  = dataArray[leftPaneIndex][colorIndex][panelIndex][rowIndex][leftByteIndex] >> ( 8 - leftShiftIndex );
        tempRightNew = dataArray[rightPaneIndex][colorIndex][panelIndex][rowIndex][rightByteIndex] >> ( 8 - leftShiftIndex );

        // now bitwise "or" them together
        if ( currentFrame <= 7 )
        {
          // Nothing should be in the left half for the first seven frames
          dataArray[2][colorIndex][panelIndex][rowIndex][0] = B00000000;
        }
        else
        {
          dataArray[2][colorIndex][panelIndex][rowIndex][0] = tempLeftOriginal | tempLeftNew;
        }
        
        dataArray[2][colorIndex][panelIndex][rowIndex][1] = tempRightOriginal | tempRightNew;
      }
    }
  }  
}

byte mod( byte dividend, byte divisor )
{
  return (byte) dividend % divisor;  
}

void loadHelix( byte pPanelPairId )
{
  int colorIndex = 0;   // 0=RED, 1=GREEN, 2=BLUE, 3=YELLOW

  // loop through all helix animation frames
  for ( int inx = 0; inx < NUM_HELIX_FRAMES; inx++ )
  {
    int helixRowIndex1 = ( pPanelPairId - 1 ) * NUM_PANELS;
    int helixRowIndex2 = helixRowIndex1 + 1;
    
    for ( int jnx = 0; jnx < NUM_ROWS; jnx++ )
    {
      // clear out the crap that was in there
      dataAssign( pPanelPairId, inx, 0, 0, jnx, B00000000, B00000000 );
      dataAssign( pPanelPairId, inx, 0, 1, jnx, B00000000, B00000000 );
      dataAssign( pPanelPairId, inx, 1, 0, jnx, B00000000, B00000000 );
      dataAssign( pPanelPairId, inx, 1, 1, jnx, B00000000, B00000000 );
      dataAssign( pPanelPairId, inx, 2, 0, jnx, B00000000, B00000000 );
      dataAssign( pPanelPairId, inx, 2, 1, jnx, B00000000, B00000000 );

      int frameLayerSum = inx + jnx;
    
      if ( frameLayerSum >= NUM_ROWS )
      {
        frameLayerSum = frameLayerSum - NUM_ROWS;
      }

      if ( frameLayerSum >= 0 && frameLayerSum < 4 )
      {
        colorIndex = 0;
      }
      else if ( frameLayerSum >= 4 && frameLayerSum < 8 )
      {
        colorIndex = 1;
      }
      else if ( frameLayerSum >= 8 && frameLayerSum < 12 )
      {
        colorIndex = 2;  
      }      
      else if ( frameLayerSum >= 12 && frameLayerSum < 16 )
      {
        colorIndex = 3;
      }

      if ( colorIndex != 3 ) // if one of the primary colors
      {      
        if ( inx + jnx < NUM_HELIX_FRAMES )
        {
          dataAssign( pPanelPairId, inx, colorIndex, 0, jnx,
                      pgm_read_byte_near( helixSeed[inx + jnx][helixRowIndex1] ),
                      pgm_read_byte_near( helixSeed[inx + jnx][helixRowIndex1] + 1 )
                     );
          dataAssign( pPanelPairId, inx, colorIndex, 1, jnx,
                      pgm_read_byte_near( helixSeed[inx + jnx][helixRowIndex2] ),
                      pgm_read_byte_near( helixSeed[inx + jnx][helixRowIndex2] + 1 )
                    );
        }
        else
        {
          dataAssign( pPanelPairId, inx, colorIndex, 0, jnx,
                      pgm_read_byte_near( helixSeed[inx + jnx - NUM_HELIX_FRAMES][helixRowIndex1] ),
                      pgm_read_byte_near( helixSeed[inx + jnx - NUM_HELIX_FRAMES][helixRowIndex1] + 1 )
                    );
          dataAssign( pPanelPairId, inx, colorIndex, 1, jnx,
                      pgm_read_byte_near( helixSeed[inx + jnx - NUM_HELIX_FRAMES][helixRowIndex2] ),
                      pgm_read_byte_near( helixSeed[inx + jnx - NUM_HELIX_FRAMES][helixRowIndex2] + 1 )
                    );
        }
      }
      else  // do yellow
      {
        if ( inx + jnx < NUM_HELIX_FRAMES )
        {
          dataAssign( pPanelPairId, inx, 0, 0, jnx,
                      pgm_read_byte_near( helixSeed[inx + jnx][helixRowIndex1] ),
                      pgm_read_byte_near( helixSeed[inx + jnx][helixRowIndex1] + 1 )
                    );
          dataAssign( pPanelPairId, inx, 0, 1, jnx,
                      pgm_read_byte_near( helixSeed[inx + jnx][helixRowIndex2] ),
                      pgm_read_byte_near( helixSeed[inx + jnx][helixRowIndex2] + 1 )
                    );
          dataAssign( pPanelPairId, inx, 1, 0, jnx,
                      pgm_read_byte_near( helixSeed[inx + jnx][helixRowIndex1] ),
                      pgm_read_byte_near( helixSeed[inx + jnx][helixRowIndex1] + 1 )
                    );
          dataAssign( pPanelPairId, inx, 1, 1, jnx,
                      pgm_read_byte_near( helixSeed[inx + jnx][helixRowIndex2] ),
                      pgm_read_byte_near( helixSeed[inx + jnx][helixRowIndex2] + 1 )
                    );
        }
        else
        {
          dataAssign( pPanelPairId, inx, 0, 0, jnx,
                      pgm_read_byte_near( helixSeed[inx + jnx - NUM_HELIX_FRAMES][helixRowIndex1] ),
                      pgm_read_byte_near( helixSeed[inx + jnx - NUM_HELIX_FRAMES][helixRowIndex1] + 1 )
                    );
          dataAssign( pPanelPairId, inx, 0, 1, jnx,
                      pgm_read_byte_near( helixSeed[inx + jnx - NUM_HELIX_FRAMES][helixRowIndex2] ),
                      pgm_read_byte_near( helixSeed[inx + jnx - NUM_HELIX_FRAMES][helixRowIndex2] + 1 )
                    );
          dataAssign( pPanelPairId, inx, 1, 0, jnx,
                      pgm_read_byte_near( helixSeed[inx + jnx - NUM_HELIX_FRAMES][helixRowIndex1] ),
                      pgm_read_byte_near( helixSeed[inx + jnx - NUM_HELIX_FRAMES][helixRowIndex1] + 1 )
                    );
          dataAssign( pPanelPairId, inx, 1, 1, jnx,
                      pgm_read_byte_near( helixSeed[inx + jnx - NUM_HELIX_FRAMES][helixRowIndex2] ),
                      pgm_read_byte_near( helixSeed[inx + jnx - NUM_HELIX_FRAMES][helixRowIndex2] + 1 )
                    );
        }
      }
    }
  }
}

void loadBlueSineWaves( byte pPanelPairId )
{
  // byte tempBuffer[MAX_FRAMES][NUM_COLORS][NUM_PANELS][NUM_ROWS][BYTES_PER_ROW];
  // byte sineWaveSeed[NUM_SINE_WAVE_FRAMES][NUM_ROWS][BYTES_PER_ROW];

  byte adjustedFrameIndex1;
  byte adjustedFrameIndex2;

  // just do a single blue wave for now ...
  for ( byte inx = 0; inx < 16; inx++ )
  {
    for ( byte jnx = 0; jnx < NUM_ROWS; jnx++ )
    {
      // clear out the crap that was in there
      dataAssign( pPanelPairId, inx + 16, 0, 0, jnx, B00000000, B00000000 );
      dataAssign( pPanelPairId, inx + 16, 0, 1, jnx, B00000000, B00000000 );
      dataAssign( pPanelPairId, inx + 16, 1, 0, jnx, B00000000, B00000000 );
      dataAssign( pPanelPairId, inx + 16, 1, 1, jnx, B00000000, B00000000 );
      dataAssign( pPanelPairId, inx + 16, 2, 0, jnx, B00000000, B00000000 );
      dataAssign( pPanelPairId, inx + 16, 2, 1, jnx, B00000000, B00000000 );

      adjustedFrameIndex1 = inx + (( pPanelPairId - 1 )) * 2;
      adjustedFrameIndex2 = inx + (( pPanelPairId - 1 )) * 2 + 1;
      
      if ( adjustedFrameIndex1 >= 16 )
      {
        adjustedFrameIndex1 = adjustedFrameIndex1 - 16;
      }
      
      if ( adjustedFrameIndex2 >= 16 )
      {
        adjustedFrameIndex2 = adjustedFrameIndex2 - 16;
      }

      dataAssign( pPanelPairId, inx + 16, 2, 0, jnx,
                  pgm_read_byte_near( sineWaveSeed[adjustedFrameIndex1][jnx] ),
                  pgm_read_byte_near( sineWaveSeed[adjustedFrameIndex1][jnx] + 1 )
                );

      dataAssign( pPanelPairId, inx + 16, 2, 1, jnx,
                  pgm_read_byte_near( sineWaveSeed[adjustedFrameIndex2][jnx] ),
                  pgm_read_byte_near( sineWaveSeed[adjustedFrameIndex2][jnx] + 1 )
                );
    }
  }
}

void loadIntersectingPlanes( byte pPanelPairId )
{
  for ( int frameIndex = 0; frameIndex < 16; frameIndex++ )
  {
    for ( int rowIndex = 0; rowIndex < 16; rowIndex++ )
    { 
      // clear out the crap that was there
      dataAssign( pPanelPairId, frameIndex, 0, 0, rowIndex, B00000000, B00000000 );
      dataAssign( pPanelPairId, frameIndex, 0, 1, rowIndex, B00000000, B00000000 );
      dataAssign( pPanelPairId, frameIndex, 1, 0, rowIndex, B00000000, B00000000 );
      dataAssign( pPanelPairId, frameIndex, 1, 1, rowIndex, B00000000, B00000000 );
      dataAssign( pPanelPairId, frameIndex, 2, 0, rowIndex, B00000000, B00000000 );
      dataAssign( pPanelPairId, frameIndex, 2, 1, rowIndex, B00000000, B00000000 );

      // start with a vertical red plane going across all panels
      dataAssign( pPanelPairId, frameIndex, 0, 0, rowIndex,
                  pgm_read_byte_near( verticalPatternSeed[frameIndex] ),
                  pgm_read_byte_near( verticalPatternSeed[frameIndex] + 1 )
                );
                
      dataAssign( pPanelPairId, frameIndex, 0, 1, rowIndex,
                  pgm_read_byte_near( verticalPatternSeed[frameIndex] ),
                  pgm_read_byte_near( verticalPatternSeed[frameIndex] + 1 )
                );

      // now do a horizontal green plane going vertical
      byte data = B00000000;
      
      if ( frameIndex == rowIndex )
      {
        data = B11111111;
      }
        
      dataAssign( pPanelPairId, frameIndex, 1, 0, rowIndex, data, data );
      dataAssign( pPanelPairId, frameIndex, 1, 1, rowIndex, data, data );
      
      // finally, a vertical blue plane moving front to back

      if ( frameIndex == ( pPanelPairId - 1 ) * 2 ||
           frameIndex == ( pPanelPairId - 1 ) * 2 + 1
         )
      {
        if ( frameIndex % 2 == 0 )
        {
          dataAssign( pPanelPairId, frameIndex, 2, 0, rowIndex, B11111111, B11111111 );
          dataAssign( pPanelPairId, frameIndex, 2, 1, rowIndex, B00000000, B00000000 );
        }
        else
        {
          dataAssign( pPanelPairId, frameIndex, 2, 0, rowIndex, B00000000, B00000000 );
          dataAssign( pPanelPairId, frameIndex, 2, 1, rowIndex, B11111111, B11111111 );
        }
      }
    }
  }    
}

void loadSineWaveRibbons( byte pPanelPairId )
{
  for ( byte inx = 0; inx < 16; inx++ )
  {
    for ( byte jnx = 0; jnx < NUM_ROWS; jnx++ )
    {
      // clear out the crap that was in there already
      dataAssign( pPanelPairId, inx + 16, 0, 0, jnx, B00000000, B00000000 );
      dataAssign( pPanelPairId, inx + 16, 0, 1, jnx, B00000000, B00000000 );
      dataAssign( pPanelPairId, inx + 16, 1, 0, jnx, B00000000, B00000000 );
      dataAssign( pPanelPairId, inx + 16, 1, 1, jnx, B00000000, B00000000 );
      dataAssign( pPanelPairId, inx + 16, 2, 0, jnx, B00000000, B00000000 );
      dataAssign( pPanelPairId, inx + 16, 2, 1, jnx, B00000000, B00000000 );

      if ( pPanelPairId == 1 || pPanelPairId == 2 )   // red going forward
      {
        dataAssign( pPanelPairId, inx + 16, 0, 0, jnx,
                    pgm_read_byte_near( sineWaveSeed[inx][jnx] ),
                    pgm_read_byte_near( sineWaveSeed[inx][jnx] + 1 )
                  );

        dataAssign( pPanelPairId, inx + 16, 0, 1, jnx,
                    pgm_read_byte_near( sineWaveSeed[inx][jnx] ),
                    pgm_read_byte_near( sineWaveSeed[inx][jnx] + 1 )
                  );
      }
      else if ( pPanelPairId == 3 || pPanelPairId == 4 )  // yellow going backward
      {
        // reds
        dataAssign( pPanelPairId, inx + 16, 0, 0, jnx,
                    pgm_read_byte_near( sineWaveSeed[15 - inx][jnx] ),
                    pgm_read_byte_near( sineWaveSeed[15 - inx][jnx] + 1 )
                  );

        dataAssign( pPanelPairId, inx + 16, 0, 1, jnx,
                    pgm_read_byte_near( sineWaveSeed[15 - inx][jnx] ),
                    pgm_read_byte_near( sineWaveSeed[15 - inx][jnx] + 1 )
                  );

        // greens
        dataAssign( pPanelPairId, inx + 16, 1, 0, jnx,
                    pgm_read_byte_near( sineWaveSeed[15 - inx][jnx] ),
                    pgm_read_byte_near( sineWaveSeed[15 - inx][jnx] + 1 )
                  );
                  
        dataAssign( pPanelPairId, inx + 16, 1, 1, jnx,
                    pgm_read_byte_near( sineWaveSeed[15 - inx][jnx] ),
                    pgm_read_byte_near( sineWaveSeed[15 - inx][jnx] + 1 )
                  );
      }
      else if ( pPanelPairId == 5 || pPanelPairId == 6 )  // green going forward
      {
