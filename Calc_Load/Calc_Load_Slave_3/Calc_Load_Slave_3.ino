    drawBox(
      6,              // byte pColor      // 1=RED, 2=GREEN, 3=BLUE, 4=YELLOW (1&2), 5=CYAN (2&3), 6=MAGENTA (1&3), 7=WHITE (1&2&3)
      frameIndex,     // byte pFrameIndex
